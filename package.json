{
  "name": "vuejs-tour",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "contributor": "git-contributor"
  },
  "dependencies": {
    "@handsontable-pro/vue": "^3.1.0",
    "element-ui": "^2.4.5",
    "handsontable-pro": "^6.2.0",
    "jquery": "^3.4.0",
    "katex": "^0.10.1",
    "mavon-editor": "^2.7.",
    "pdfvuer": "^1.3.0",
    "vue": "^2.6.10",
    "vue-katex": "^0.2.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-runtime": "^6.26.0",
    "git-contributor": "^1.0.10",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.11.0",
    "pdfjs-dist": "^2.0.943",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-loader": "^15.7.0",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
